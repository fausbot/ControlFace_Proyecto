rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /asistencia/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /asistencias/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /incidentes/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
